{
	"luau-lsp.require.mode": "relativeToFile",
	"luau-lsp.sourcemap.enabled": true,
	"luau-lsp.sourcemap.autogenerate": false,
	"luau-lsp.sourcemap.rojoProjectFile": "default.project.json",
	"luau-lsp.platform.type": "roblox",
	"luau-lsp.suggest.enabled": true,
	"luau-lsp.completion.enabled": true,
	"luau-lsp.completion.fillCallParens": true,
	"luau-lsp.completion.addParentheses": true,
	"luau-lsp.hover.enabled": true,
	"luau-lsp.diagnostics.enabled": true,
	"luau-lsp.diagnostics.strictDatamodelTypes": true,
	"files.associations": {
		"*.lua": "luau",
		"*.luau": "luau"
	},
	"editor.defaultFormatter": "JohnnyMorganz.stylua",
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll": "explicit"
	},
	"[luau]": {
		"editor.defaultFormatter": "JohnnyMorganz.stylua",
		"editor.formatOnSave": true,
		"editor.insertSpaces": false,
		"editor.tabSize": 4
	},
	"[markdown]": {
		"editor.defaultFormatter": "vscode.markdown-language-features"
	},
	"luau-lsp.diagnostics.workspace": true,
	"luau-lsp.completion.enableFragmentAutocomplete": true,
	"luau-lsp.completion.imports.enabled": true,
	"luau-lsp.completion.showPropertiesOnMethodCall": true,
	"luau-lsp.fflags.enableByDefault": true,
	"luau-lsp.fflags.enableNewSolver": true,
	"luau-lsp.hover.showTableKinds": true,
	"luau-lsp.inlayHints.functionReturnTypes": true,
	"luau-lsp.inlayHints.hideHintsForMatchingParameterNames": false,
	"luau-lsp.inlayHints.parameterNames": "all",
	"luau-lsp.inlayHints.parameterTypes": true,
	"luau-lsp.inlayHints.typeHintMaxLength": 500,
	"luau-lsp.inlayHints.variableTypes": true,
	"luau-lsp.plugin.enabled": true,
	"notebook.defaultFormatter": "ms-toolsai.jupyter",
	"[yaml]": {
		"editor.defaultFormatter": "redhat.vscode-yaml"
	}
}